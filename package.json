{
  "name": "ccxt-native",
  "version": "1.0.0",
  "description": "ccxt for Swift, Obj-C and Java/Kotlin applications",
  "scripts": {
    "buildccxt": "cd .. && npm run build && cd ccxt-native",
    "buildGO": "cd .. && npm run buildGO && cd ccxt-native",
    "formatGO": "cd .. && npm run formatGO && cd ccxt-native",
    "transpileGO": "cd .. && npm run transpileGO && cd ccxt-native",
    "transpileGORest": "cd .. && npm run transpileGORest && cd ccxt-native",
    "transpileGOWs": "cd .. && npm run transpileGOWs && cd ccxt-native",

    "build": "npm run transpile && npm run buildSwift && npm run buildAndroid",
    "buildAndroid": "echo \"TODO: buildAndroid\"",
    "buildSwift": "tsx main/build/buildXcframework.ts",
    "cleanGOmobile": "./main/build/goPatches/cleanGomobileCache.sh",
    "patchGO": "./main/build/goPatches/goPatch.sh",
    "test": "echo Error: no test specified && exit 1",
    "transpile": "npm run transpileSwift && npm run transpileGOWrapper && npm run transpileAndroid",
    "transpileAndroid": "tsx main/build/transpileAndroid.ts",
    "transpileGOWrapper": "tsx main/build/transpileGoWrapper.ts",
    "transpileSwift": "tsx main/build/transpileSwift.ts"
  },
